<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CampusWeb"
        android:id="@+id/CampusWeb"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Academics"
        android:id="@+id/Academics"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/CampusWeb"
        android:layout_toEndOf="@+id/CampusWeb"
        android:onClick="AcademicsClick"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Athletics"
        android:id="@+id/Athletics"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Food"
        android:id="@+id/Food"
        android:layout_below="@+id/CampusWeb"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Directory"
        android:id="@+id/Directory"
        android:layout_alignTop="@+id/Food"
        android:layout_toRightOf="@+id/Food"
        android:layout_toEndOf="@+id/Food" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="eRaven"
        android:id="@+id/eRaven"
        android:layout_alignTop="@+id/Directory"
        android:layout_alignRight="@+id/Athletics"
        android:layout_alignEnd="@+id/Athletics" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendar"
        android:id="@+id/Calendar"
        android:layout_below="@+id/Food"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emergency"
        android:id="@+id/Emergency"
        android:layout_alignTop="@+id/Calendar"
        android:layout_toRightOf="@+id/Calendar"
        android:layout_toEndOf="@+id/Calendar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="StudentLife"
        android:id="@+id/StudentLife"
        android:layout_alignBottom="@+id/Emergency"
        android:layout_alignRight="@+id/eRaven"
        android:layout_alignEnd="@+id/eRaven" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Social"
        android:id="@+id/Social"
        android:layout_below="@+id/Calendar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="41dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Library"
        android:id="@+id/Library"
        android:layout_alignTop="@+id/Social"
        android:layout_toRightOf="@+id/Social"
        android:layout_toEndOf="@+id/Social" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Security"
        android:id="@+id/Security"
        android:layout_alignTop="@+id/Library"
        android:layout_alignRight="@+id/StudentLife"
        android:layout_alignEnd="@+id/StudentLife" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="News"
        android:id="@+id/News"
        android:layout_below="@+id/Social"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="41dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maps"
        android:id="@+id/Maps"
        android:layout_alignTop="@+id/News"
        android:layout_toRightOf="@+id/News"
        android:layout_toEndOf="@+id/News" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bill Pay"
        android:id="@+id/button15"
        android:layout_alignTop="@+id/Maps"
        android:layout_alignRight="@+id/Security"
        android:layout_alignEnd="@+id/Security" />
</RelativeLayout>
